{
    "compilerOptions": {
      "target": "ESNext",      
      "module": "ESNext",      
      "outDir": "./dist",      
      "esModuleInterop": true, 
      "strict": true,          
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "skipLibCheck": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  